{
  "name": "rx-util",
  "version": "1.7.10",
  "description": "rxliuli 个人使用的 js 工具库（仅适用于浏览器）",
  "main": "./dist/rx-util.js",
  "module": "./dist/rx-util-es.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "start": "yarn test",
    "build": "rollup -c build/rollup.config.js",
    "pack:add": "yarn link && yarn link \"rx-util\"",
    "pack:remove": "yarn unlink \"rx-util\" && yarn unlink",
    "lint": "tslint src/**/*.ts ",
    "format": "prettier --write src/**/*.ts src/**/*.js",
    "docs": "typedoc --out docs src --exclude src/**/*.test.ts && cp CNAME docs/ && cp readme.md docs/ && cp .nojekyll docs/",
    "cloc": "cloc src/ --exclude-ext=test.ts",
    "test": "jest --watch",
    "coverage": "jest --coverage",
    "fix": "tslint --fix src/**/*.ts",
    "server": "yarn docs && yarn coverage && live-server docs/ --port=4000",
    "deploy": "yarn gh-pages -d docs -t true && rm -rf docs",
    "prepublishOnly": "yarn build && yarn coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/rxliuli/rx-util.git"
  },
  "keywords": [
    "util",
    "browser",
    "rxliuli"
  ],
  "author": "rxliuli",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rxliuli/rx-util/issues",
    "email": "rxliuli@gmail.com"
  },
  "homepage": "https://rx-util.rxliuli.com/",
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/prettier": "^1.16.4",
    "@types/rimraf": "^2.0.2",
    "@types/rollup": "^0.54.0",
    "cloc": "^2.5.0",
    "gh-pages": "^2.0.1",
    "husky": "^1.3.1",
    "jest": "^24.8.0",
    "jest-extended": "^0.11.1",
    "lint-staged": "^8.1.5",
    "live-server": "^1.2.1",
    "prettier": "^1.17.1",
    "rimraf": "^2.6.3",
    "rollup": "^1.12.3",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-typescript2": "^0.21.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "tslib": "^1.9.3",
    "typedoc": "^0.14.2",
    "typescript": "^3.6.4"
  }
}
