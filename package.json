{
  "name": "rx-util",
  "version": "1.8.11",
  "description": "rxliuli 个人使用的 js 工具库（仅适用于浏览器）",
  "main": "./dist/index.js",
  "module": "./dist/rx-util.esm.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test -w",
    "lint": "tsdx lint --fix",
    "docs": "typedoc --options typedoc.json && cp CNAME docs/ && cp readme.md docs/ && cp .nojekyll docs/",
    "cloc": "cloc src/ --exclude-ext=test.ts",
    "coverage": "jest --coverage",
    "server": "yarn docs && yarn coverage && live-server docs/ --port=4000",
    "deploy": "yarn gh-pages -d docs -t true && rm -rf docs",
    "publishReady": "yarn format && yarn build && yarn rimraf docs && yarn docs && yarn coverage && yarn deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/rxliuli/rx-util.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "util",
    "browser",
    "rxliuli"
  ],
  "author": "rxliuli",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rxliuli/rx-util/issues",
    "email": "rxliuli@gmail.com"
  },
  "prettier": {
    "tabWidth": 2,
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "endOfLine": "lf"
  },
  "homepage": "https://rx-util.rxliuli.com/",
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "cloc": "^2.5.0",
    "eledoc": "^0.2.1",
    "gh-pages": "^2.0.1",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "jest-extended": "^0.11.1",
    "liuli-types": "^0.1.1",
    "live-server": "^1.2.1",
    "prettier": "^1.17.1",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "ts-toolbelt": "^6.9.4",
    "tsdx": "^0.13.3",
    "tslib": "^2.0.1",
    "typedoc": "^0.16.10",
    "typescript": "^3.9.7"
  }
}
