{
  "name": "rx-util",
  "version": "1.6.14-beta6",
  "description": "rxliuli 个人使用的 js 工具库（仅适用于浏览器）",
  "main": "./dist/rx-util.js",
  "module": "./dist/rx-util-es.js",
  "scripts": {
    "start": "npm run test",
    "dev": "rollup -c build/rollup.config.js --environment NODE_ENV:production --watch",
    "build:dev": "rollup -c build/rollup.config.js --environment NODE_ENV:development",
    "build:prod": "rollup -c build/rollup.config.js --environment NODE_ENV:production",
    "build": "npm run build:prod",
    "lint": "eslint --fix ./src/",
    "docs": "esdoc",
    "test": "jest --watch",
    "coverage": "jest --coverage",
    "fix": "eslint --fix src/",
    "server": "npm run docs && npm run coverage && live-server docs/ --port=4000",
    "predeploy": "yarn docs && yarn coverage && cp CNAME docs/ && cp readme.md docs/",
    "deploy": "gh-pages -d docs && rm -rf docs"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/rxliuli/rx-util.git"
  },
  "keywords": [
    "util",
    "browser",
    "rxliuli"
  ],
  "author": "rxliuli",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rxliuli/rx-util/issues",
    "email": "rxliuli@gmail.com"
  },
  "homepage": "https://rx-util.rxliuli.com/",
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.4.2",
    "@babel/runtime": "^7.4.2",
    "@types/jest": "^24.0.12",
    "core-js": "3",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^5.15.3",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "gh-pages": "^2.0.1",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "jest-extended": "^0.11.1",
    "lint-staged": "^8.1.5",
    "live-server": "^1.2.1",
    "prettier": "^1.16.4",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-eslint": "^5.0.0",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-uglify": "^6.0.1",
    "rx-util": "^1.6.13-beta1"
  }
}
