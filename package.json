{
  "name": "rx-util",
  "version": "1.7.2",
  "description": "rxliuli 个人使用的 js 工具库（仅适用于浏览器）",
  "main": "./dist/rx-util.js",
  "module": "./dist/rx-util-es.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "start": "yarn-or-npm test",
    "build": "rollup -c build/rollup.config.js",
    "lint": "tslint src/**/*.ts ",
    "format": "prettier --write src/**/*.ts src/**/*.js",
    "docs": "rimraf docs && typedoc --out docs src --exclude src/**/*.test.ts",
    "cloc": "cloc src/ --exclude-ext=test.ts",
    "test": "jest --watch",
    "coverage": "jest --coverage",
    "fix": "tslint --fix src/**/*.ts",
    "server": "yarn-or-npm docs && yarn-or-npm coverage && live-server docs/ --port=4000",
    "predeploy": "yarn-or-npm docs && yarn-or-npm coverage && cp CNAME docs/ && cp readme.md docs/ && cp .nojekyll docs/",
    "deploy": "gh-pages -d docs && rm -rf docs",
    "prepublishOnly": "yarn-or-npm build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/rxliuli/rx-util.git"
  },
  "keywords": [
    "util",
    "browser",
    "rxliuli"
  ],
  "author": "rxliuli",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rxliuli/rx-util/issues",
    "email": "rxliuli@gmail.com"
  },
  "homepage": "https://rx-util.rxliuli.com/",
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/prettier": "^1.16.4",
    "@types/rimraf": "^2.0.2",
    "@types/rollup": "^0.54.0",
    "@types/typescript": "^2.0.0",
    "cloc": "^2.5.0",
    "gh-pages": "^2.0.1",
    "husky": "^1.3.1",
    "jest": "^24.8.0",
    "jest-extended": "^0.11.1",
    "lint-staged": "^8.1.5",
    "live-server": "^1.2.1",
    "prettier": "^1.17.1",
    "rimraf": "^2.6.3",
    "rollup": "^1.12.3",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-tslint": "^0.2.2",
    "rollup-plugin-typescript2": "^0.21.1",
    "rollup-plugin-uglify": "^6.0.2",
    "rx-util": "latest",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "tslib": "^1.9.3",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typedoc": "^0.14.2",
    "typescript": "^3.4.5",
    "yarn-or-npm": "^2.0.4"
  }
}
