{
  "name": "rx-util",
  "version": "1.8.6",
  "description": "rxliuli 个人使用的 js 工具库（仅适用于浏览器）",
  "main": "./dist/rx-util.js",
  "module": "./dist/rx-util-es.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "start": "yarn test",
    "build": "rollup -c build/rollup.config.js",
    "format": "prettier --write src/**/*.ts src/**/*.js",
    "docs": "typedoc --options typedoc.json && cp CNAME docs/ && cp readme.md docs/ && cp .nojekyll docs/",
    "cloc": "cloc src/ --exclude-ext=test.ts",
    "test": "jest --watch",
    "coverage": "jest --coverage",
    "server": "yarn docs && yarn coverage && live-server docs/ --port=4000",
    "deploy": "yarn gh-pages -d docs -t true && rm -rf docs",
    "publishReady": "yarn format && yarn build && yarn rimraf docs && yarn docs && yarn coverage && yarn deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/rxliuli/rx-util.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "util",
    "browser",
    "rxliuli"
  ],
  "author": "rxliuli",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rxliuli/rx-util/issues",
    "email": "rxliuli@gmail.com"
  },
  "homepage": "https://rx-util.rxliuli.com/",
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/prettier": "^1.16.4",
    "@types/rimraf": "^2.0.2",
    "@types/rollup": "^0.54.0",
    "cloc": "^2.5.0",
    "eledoc": "^0.2.1",
    "gh-pages": "^2.0.1",
    "husky": "^1.3.1",
    "jest": "^24.8.0",
    "jest-extended": "^0.11.1",
    "lint-staged": "^8.1.5",
    "liuli-types": "^0.1.1",
    "live-server": "^1.2.1",
    "prettier": "^1.17.1",
    "rimraf": "^2.6.3",
    "rollup": "^1.12.3",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-typescript2": "^0.21.1",
    "rollup-plugin-visualizer": "^3.3.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "ts-toolbelt": "^6.9.4",
    "tslib": "^1.9.3",
    "typedoc": "^0.16.10",
    "typescript": "^3.7.2"
  }
}
